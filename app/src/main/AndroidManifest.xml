<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.github.studybuddy.rcos.studybuddy_app" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".assign_add_Fragment"
            android:label="Add New Assignment" >
        </activity>
        <activity
            android:name=".assign_add_edit_date"
            android:label="Edit Due Date" >
        </activity>
        <activity
            android:name=".assign_add_edit_time"
            android:label="Edit Due Date - Time" >
        </activity>
    </application>

</manifest>
